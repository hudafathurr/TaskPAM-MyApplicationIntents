<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/btn_pindah"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="MOVE ACTIVITY"/>
    <Button
        android:id="@+id/btn_pindah_data"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="MOVING ACTIVITY WITH DATA "/>
    <Button
        android:id="@+id/btn_pindah_object"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="MOVING ACTIVITY WITH OBJECT"/>
    <Button
        android:id="@+id/btn_dial"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="DIAL A NUMBER"/>
    <Button
        android:id="@+id/btn_pindah_result"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="MOVING ACTIVITY FOR RESULT"/>
    <TextView
        android:id="@+id/tv_result"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Result from Activity"
        android:layout_gravity="center"/>


</LinearLayout>